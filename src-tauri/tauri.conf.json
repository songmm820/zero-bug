{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "zero-bug",
  "version": "0.1.0",
  "identifier": "com.zero-bug.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "zero-bug",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon-32.png",
      "icons/icon-32@2x.png",
      "icons/icon-128.png",
      "icons/icon-128@2x.png",
      "icons/icon-256.png",
      "icons/icon-256@2x.png",
      "icons/icon-512.png",
      "icons/icon-512@2x.png",
      "icons/icon.ico"
    ]
  }
}
